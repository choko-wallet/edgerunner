// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Event {
    id        String   @id @default(cuid())
    startTime DateTime
    endTime   DateTime
    eventName String
    hostList  String
    locationText String
    locationGoogle String
    tweet String
    hashTag String
}
